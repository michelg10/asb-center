view {
    --button-background-color: #FDFEFD;
    --subcontainer-background: #FFFFFE;
    --link: #007AFF;
    --blue: #007AFF;
    --secondary: #959599;
    --list-separator: #E7E7E7;
    --list-search-box: #E4E3E9;
    --warn: #FF3B30;
    --red: #FF3B30;
    --green: #34C759;
    --page-background: #F2F2F6;
    --primary: #000000;
    --anti-primary: #FFFFFE;
}
@media (prefers-color-scheme: dark) {
    view {
        --button-background-color: #1C1C1E;
        --subcontainer-background: #1C1C1E;
        --link: #0A84FF;
        --blue: #0A84FF;
        --secondary: #6C6C6C;
        --list-separator: #383838;
        --list-search-box: #1C1C1E;
        --warn: #FF453A;
        --red: #FF453A;
        --green: #30D158;
        --page-background: #000000;
        --primary: #FFFFFE;
        --anti-primary: #000000;
    }
}